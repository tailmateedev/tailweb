---
import '../style/main.css'
import CircleMedia from '@maintenance/components/CircleMedia.vue';
import Header from '@maintenance/components/Header.vue';

interface Props {
	title: string;
}

const { title } = Astro.props;

---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/tailmatee-logo.jpeg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body class="bg-gray-100 h-screen overflow-hidden flex flex-col">
		<header>
		  <Header client:load />
		</header>
		<div class="flex flex-col items-center justify-center flex-grow text-center mt-[50px]">
			<h1 class="text-[90px] font-semibold mt-[-120px] text-[#11113A] font-montserrat tracking-normal">
				Próximamente . . .
			</h1>
			<h3 class="text-[30px] mt-[20px] text-[#11113A] font-montserrat tracking-normal">
			  Síguenos en redes y mantente al <br> tanto de nuestras novedades
			</h3>
		</div>
		<div class="absolute bottom-4 right-4 flex flex-row-reverse space-x-4">
			<CircleMedia 
